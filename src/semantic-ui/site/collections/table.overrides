/*******************************
         Site Overrides
*******************************/

.ui.table.rowspanStriped > tbody:nth-child(2n+1) {
  background-color: rgba(0, 0, 50, 0.02);
}

* Peach */
.ui.peach.table {
  border-top: @coloredBorderSize solid @peach;
  color: @peach !important;
}

.ui.inverted.peach.table {
  background-color: @peach !important;
  color: @white !important;
}
